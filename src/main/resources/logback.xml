<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%msg%n</pattern>
		</encoder>
	</appender>
	
	<!-- loggerse -->
	<logger name="dev.banque.app" level="INFO" additivity="false">
        <appender-ref ref="STDOUT" />
    </logger>
</configuration>